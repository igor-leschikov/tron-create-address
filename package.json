{
  "name": "tron-create-address",
  "version": "0.1.11",
  "description": "Tron address and private key generator",
  "main": "build/lib/account.js",
  "types": "build/lib/account.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "npx mocha -r ts-node/register 'test/**/*.ts'",
    "build": "npx tsc",
    "lint": "npx eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "npx eslint '*/**/*.{ts,tsx}' --fix",
    "format": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kushkamisha/tron-create-address.git"
  },
  "keywords": [
    "tron",
    "address",
    "account",
    "private key",
    "generator"
  ],
  "author": "Misha Kushka",
  "license": "MIT",
  "homepage": "https://github.com/kushkamisha/tron-create-address#readme",
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.4",
    "@types/elliptic": "^6.4.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "js-sha3": "^0.8.0"
  },
  "bugs": {
    "url": "https://github.com/kushkamisha/tron-create-address/issues"
  }
}
